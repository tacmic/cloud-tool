{
    "AWSTemplateFormatVersion": "2010-09-09",
    "Resources": {
        "production": {
            "Type": "AWS::EC2::VPC",
            "Properties": {
                "CidrBlock": "13.0.0.0/16",
                "EnableDnsSupport": "true",
                "EnableDnsHostnames": "false",
                "InstanceTenancy": "default",
                "Tags": [
                    {
                        "Key": "Name",
                        "Value": "prd-tky"
                    }
                ]
            }
        },
        "staging": {
            "Type": "AWS::EC2::VPC",
            "Properties": {
                "CidrBlock": "13.1.0.0/16",
                "EnableDnsSupport": "true",
                "EnableDnsHostnames": "false",
                "InstanceTenancy": "default",
                "Tags": [
                    {
                        "Key": "Name",
                        "Value": "stg-tky"
                    }
                ]
            }
        },
        "prdPublicA": {
            "Type": "AWS::EC2::Subnet",
            "Properties": {
                "VpcId": {
                    "Ref": "production"
                },
                "CidrBlock": "13.0.0.0/19",
